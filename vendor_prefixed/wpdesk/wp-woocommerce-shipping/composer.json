{
    "name": "wpdesk\/wp-woocommerce-shipping",
    "description": "WooCommerce Shipping",
    "license": "MIT",
    "keywords": [
        "wordpress",
        "woocommerce",
        "shipping"
    ],
    "homepage": "https:\/\/gitlab.com\/wpdesk\/wp-woocommerce-shipping",
    "authors": [
        {
            "name": "grola",
            "email": "grola@wpdesk.net"
        }
    ],
    "config": {
        "gitlab-domains": [
            "gitlab.wpdesk.dev"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "ext-json": "*",
        "php": ">=7.4",
        "wpdesk\/abstract-shipping": "^2.8.0",
        "psr\/log": "^1.1",
        "wpdesk\/wp-view": "^1.1|^2.0",
        "wpdesk\/wp-plugin-flow-common": "^1.0.3",
        "wpdesk\/wp-persistence": "^3.0",
        "wpdesk\/wp-show-decision": "^1.0",
        "wpdesk\/wp-notice": "^3.2",
        "octolize\/wp-octolize-brand-assets": "^1.3"
    },
    "require-dev": {
        "phpunit\/phpunit": "^7||^8||^9",
        "wp-coding-standards\/wpcs": "^0.14.1",
        "squizlabs\/php_codesniffer": "^3.0.2",
        "mockery\/mockery": "*",
        "10up\/wp_mock": "*",
        "phpcompatibility\/php-compatibility": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "OctolizeShippingRoyalMailVendor\\WPDesk\\WooCommerceShipping\\": "src\/WooCommerceShipping"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests\/unit"
        ]
    },
    "extra": {
        "text-domain": "wp-woocommerce-shipping",
        "translations-folder": "lang",
        "po-files": {
            "pl_PL": "pl_PL.po",
            "en_AU": "en_AU.po",
            "en_CA": "en_CA.po",
            "en_GB": "en_GB.po",
            "de_DE": "de_DE.po"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpunit-unit": "phpunit --configuration phpunit-unit.xml --coverage-text --colors=never",
        "phpunit-unit-fast": "phpunit --configuration phpunit-unit.xml --no-coverage",
        "phpunit-integration": "phpunit --configuration phpunit-integration.xml --coverage-text --colors=never",
        "phpunit-integration-fast": "phpunit --configuration phpunit-integration.xml --no-coverage"
    },
    "repositories": {
        "octolize": {
            "type": "composer",
            "url": "https:\/\/gitlab.wpdesk.dev\/api\/v4\/group\/wpdesk\/-\/packages\/composer\/"
        },
        "wpdesk": {
            "type": "composer",
            "url": "https:\/\/gitlab.wpdesk.dev\/api\/v4\/group\/wpdesk\/-\/packages\/composer\/"
        }
    }
}